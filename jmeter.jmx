            <stringProp name="script">import java.util.UUID;
import org.apache.jmeter.threads.JMeterContextService;

// Obtém o número de thread atual
int threadNum = JMeterContextService.getContext().getThreadNum();

// Calcula os valores de &apos;rules&apos; e &apos;profileId&apos; com base no número da thread
int rulesValue = (threadNum % 10) + 1; // Garante valores de 1 a 10
int profileIdValue = ((threadNum + 5) % 10) + 1; // Garante valores de 1 a 10 (deslocados)

// Armazena os UUIDs nas variáveis JMeter
vars.put(&quot;rules&quot;,String.valueOf(rulesValue));
vars.put(&quot;profileId&quot;, String.valueOf(profileIdValue));


log.info(&quot;Thread: &quot; + threadNum + &quot;, rules: &quot; + rulesValue);
log.info(&quot;Thread: &quot; + threadNum + &quot;, profileId: &quot; + profileIdValue);</stringProp>
